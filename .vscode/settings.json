{
    "editor.formatOnSave": false,
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.fixAll.eslint": true
    },
    "eslint.validate": ["javascript"],
    "cSpell.words": [
        "linebreak",
        "plusplus"
    ]
}