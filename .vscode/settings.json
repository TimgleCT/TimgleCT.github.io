{
    "editor.formatOnSave": false,
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[vue]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.fixAll.eslint": true
    },
    "eslint.validate": ["javascript", "vue"],
    "vetur.validation.template": false,
    "cSpell.words": [
        "linebreak",
        "plusplus"
    ]
}